{
  "ConnectionStrings": {
    "SqlServer": "Server=${SQL_SERVER};Database=VeniceOrders_Prod;User Id=${SQL_USER};Password=${SQL_PASSWORD};TrustServerCertificate=False;",
    "MongoDb": "${MONGODB_CONNECTION}",
    "Redis": "${REDIS_CONNECTION}",
    "RabbitMQ": "${RABBITMQ_CONNECTION}"
  },
  "Jwt": {
    "SecretKey": "${JWT_SECRET_KEY}",
    "Issuer": "VeniceOrders",
    "Audience": "VeniceOrders",
    "ExpirationMinutes": 60
  },
  "Cache": {
    "DefaultExpirationMinutes": 2
  },
  "MongoDb": {
    "DatabaseName": "VeniceOrders_Prod"
  },
  "RabbitMQ": {
    "ExchangeName": "venice.orders",
    "QueueName": "pedido.criado"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}

